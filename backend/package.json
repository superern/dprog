{
  "name": "dprog-backend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "serverless package",
    "deploy": "serverless deploy",
    "offline": "serverless offline",
    "localstack:start": "docker run --rm -it -p 4566:4566 -e SERVICES=s3,sqs -e AWS_DEFAULT_REGION=us-east-1 localstack/localstack",
    "localstack:init": "AWS_ACCESS_KEY_ID=test AWS_SECRET_ACCESS_KEY=test AWS_REGION=us-east-1 aws --endpoint-url http://localhost:4566 s3 mb s3://dprog && AWS_ACCESS_KEY_ID=test AWS_SECRET_ACCESS_KEY=test AWS_REGION=us-east-1 aws --endpoint-url http://localhost:4566 sqs create-queue --queue-name ingest-q"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.637.0",
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "@pinecone-database/pinecone": "^2.0.1",
    "openai": "^4.56.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.137",
    "@types/node": "^20.14.12",
    "esbuild": "^0.23.1",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.54.4",
    "serverless-offline": "^12.0.0",
    "serverless-offline-sqs": "^7.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "typescript": "^5.5.4"
  }
}
